{
  "name": "zod-tools",
  "description": "Tools for working with Zod schemas.",
  "version": "0.2.0",
  "main": "dist/lib/index.js",
  "exports": {
    ".": {
      "require": "./dist/lib/index.js",
      "import": "./dist/lib/index.js",
      "default": "./dist/lib/index.js",
      "types": "./dist/lib/index.d.ts"
    }
  },
  "typings": "dist/lib/index",
  "repository": "github:gregros/zod-tools",
  "scripts": {
    "build": "tsc -b tsconfig.all.json",
    "clean": "shx rm -rf dist",
    "rebuild": "run-s clean build",
    "prepare": "husky install",
    "lint:fix": "prettier --write src/**/*.ts && eslint --fix src/**/*.ts",
    "lint:check": "prettier --check src/**/*.ts && eslint src/**/*.ts",
    "test": "jest --coverage"
  },
  "keywords": [
    "zod",
    "schema",
    "traverse",
    "transform",
    "match",
    "visitor",
    "utility"
  ],
  "author": "GregRos",
  "license": "MIT",
  "lint-staged": {
    "*.{ts,js,mjs,mts,cjs,cts}": "eslint --cache --fix",
    "*.{ts,js,mts,mjs,cts,cjs,css,md,yml,yaml,json}": "prettier --write"
  },
  "devDependencies": {
    "@gregros/eslint-config": "^0.4.0",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.198",
    "@types/node": "^20.6.3",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "zod": "^3.22.2"
  },
  "peerDependencies": {
    "zod": "*"
  },
  "dependencies": {
  }
}
